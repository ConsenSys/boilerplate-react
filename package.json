{
	"name": "boilerplate-react",
	"version": "0.3.0-dev",
	"description": "React boilerplate",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com:ConsenSys/boilerplate-react.git"
	},
	"keywords": [
		"react",
		"boilerplate",
		"consensys"
	],
	"author": "ConsenSys France",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ConsenSys/boilerplate-react/issues"
	},
	"dependencies": {
		"@material-ui/core": "^4.1.2",
		"@material-ui/icons": "^4.2.1",
		"clsx": "^1.0.4",
		"connected-react-router": "^6.4.0",
		"history": "^4.9.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-redux": "^7.1.0",
		"react-router-dom": "^5.0.1",
		"react-scripts": "3.0.1",
		"redux": "^4.0.1",
		"reselect": "^4.0.0"
	},
	"devDependencies": {
		"enzyme": "^3.10.0",
		"enzyme-adapter-react-16": "^1.14.0",
		"enzyme-to-json": "^3.3.5",
		"husky": "^2.4.1",
		"lint-staged": "^8.2.1",
		"prettier": "^1.18.2",
		"redux-test-utils": "^0.3.0"
	},
	"lint-staged": {
		"src/**/*.{js,jsx,json,css}": [
			"prettier --write",
			"git add"
		]
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx}",
			"!**/src/index.js",
			"!**/src/registerServiceWorker.js"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"prettier": {
		"useTabs": false,
		"tabWidth": 4
	}
}
